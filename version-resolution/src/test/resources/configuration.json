{
  "bom-coordinates": "test:test:0.1",
  "ignoreScopes": ["test"],
  "rules": {
    "org.wildfly:wildfly-messaging": {
      "STREAM": "MINOR"
    },
    "org.picketlink:*": {
      "STREAM": "QUALIFIER",
      "QUALIFIER": [
        "Final",
        "SP\\d+"
      ]
    },
    "org.wildfly:wildfly-core": {
      "PREFIX": "10.0.0",
      "QUALIFIER": "Beta\\d+"
    },
    "*:*": "MICRO"
  }
}
